package dhiraj;

class Sample {
     void m4(){
        System.out.println("I am first1");
    }
     void m2(){
        
        System.out.println("I am Second3");
        m4();
    }
} 
class Test extends Sample{
    void m1(){
        System.out.println("I am first776");
    }
    void m3(){
        System.out.println("I am first2");
        m4();
        super.m2();
    }
} 

class Example extends Test{
    void m2(){
        System.out.println("I am first776");
        
    }
}

public class Main extends Example
{
    void m4(){
        System.out.println("I am first776");
        
    }
    void m1(){
        System.out.println("I am first7766666");
        
    }
    void m2(){
        System.out.println("I am first776");
        
    }
    
   
	public static void main(String[] args) {
		System.out.println("Hello World");
		
		Main a2=new Main();
	    a2.m3();
		
	}
}

/*
 Hello World
I am first2
I am first776
I am Second3
I am first776
in non-static context the compiler first execute the method from subclass then if it is not
overriding then it will go for superclass 
*/ 

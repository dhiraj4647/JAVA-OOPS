package dhiraj;

class Sample {
    //Overridden method
	void m1() {
		System.out.println("I am first");
	}

	void m2() {
		System.out.println("I am Second");
		m1();
	}
    //overridden method.
	void m3() {
		System.out.println("I am superclass third method");
	}
}

public class Main extends Sample {
	//overriding method
	void m1() {

		System.out.println("I am new");
	}
    //overriding method
	void m3() {
		System.out.println("I am subclass third method");
		super.m3();// to access the overridden method use super keyword
	}

	public static void main(String[] args) {
		System.out.println("Hello World");
		Sample a1 = new Main();
		Main a2 = new Main();
		a1.m1();
		a1.m2();
		a2.m1();
		a2.m2();

		a2.m3();
	}
}

/*
Hello World
I am new
I am Second
I am new
I am new
I am Second
I am new
I am subclass third method
I am superclass third method
*/
